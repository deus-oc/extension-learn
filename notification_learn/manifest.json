{
    "manifest_version": 2,
    "name": "Notification Extension",
    "description": "notification API usecase",
    "version": "1.0",
    "icons": {
        "64": "./icons/icon.jpeg"
    },
    "permissions": ["notifications"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": {
            "64": "./icons/icon.jpeg"
        },
        "default_title": "myNotification"
    }
}